    M<- median(stations.long[(t-17):(t- 1),"Hiiraan_Belet_WeyneStation_Shabelle_River"], na.rm=TRUE)
    N<- max(0.304040139329624*I*J, 0.000178462844661094*K*L,na.rm=TRUE)
