    N<- median(future.long[(t-9):(t- 1),"Gedo_FutureRegion"], na.rm=TRUE)
    O<- future.long[(t- 1),"Galgaduud_FutureRegion"]
    P<- future.long[(t- 1),"Jubbada_Hoose_FutureRegion"]
