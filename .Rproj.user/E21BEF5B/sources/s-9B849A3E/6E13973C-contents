    if(is.infinite(J)){J <- 0 }
    if(is.infinite(O)){O <- 0 }
    if(is.infinite(N)){N <- 0 }
    FIN <-sum( 0.0732602367252228*A*B , 0.0732602367252228*C*D , 0.0732602367252228*E*G , 8.54019598974934e-6*H*I , J , O , -N,na.rm=TRUE)
    PA[t] <- FIN
