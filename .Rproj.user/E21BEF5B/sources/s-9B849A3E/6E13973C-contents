    O<- max(H, I*J,na.rm=TRUE)
    P<- max(K*L, 0.0187301275782267*M,na.rm=TRUE)
    Q<- max(129.108864706647,sum( A*B , 0.000102684111890545*C^2*D , E , G , O , P , -129.108864706647,na.rm=TRUE),na.rm=TRUE)
    if(is.infinite(Q)){Q <- 0 }
