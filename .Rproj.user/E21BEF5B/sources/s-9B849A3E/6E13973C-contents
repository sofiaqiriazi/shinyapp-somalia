    E<- conflicts.long[(t- 1),"Nugaal_Conflict"]
    G<- current.long[(t- 1),"Bay_CurrentRegion"]
    H<- current.long[(t- 1),"Hiiraan_CurrentRegion"]
    I<- current.long[(t- 1),"Togdheer_CurrentRegion"]
    J<- future.long[(t- 1),"Jubbada_Hoose_FutureRegion"]
