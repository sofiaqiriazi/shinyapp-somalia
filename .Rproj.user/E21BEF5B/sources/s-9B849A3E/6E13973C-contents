    C<- before.long[(t- 1),"Shabeellaha_Dhexe_BeforeRegion"]
    D<- tail(movavg(fatalities.long[(t-5):(t- 1),"Togdheer_Fatalities"], 4,type="m"),1)
    E<- conflicts.long[(t- 1),"Awdal_Conflict"]
    G<- fatalities.long[(t- 9),"Bari_Fatalities"]
