    L<- before.long[(t- 1),"Hiiraan_BeforeRegion"]
    M<- median(rain.long[(t-15):(t- 1),"Gedo_rain"], na.rm=TRUE)
    N<- current.long[(t- 17),"Mudug_CurrentRegion"]
