    if(is.infinite(K)){K <- 0 }
    if(is.infinite(L)){L <- 0 }
    if(is.infinite(M)){M <- 0 }
    FIN <-sum( A*B , 0.00197058875505935*C*D , 8.48543352535101e-6*E^2 , 0.0120431601069705*G*H , 0.00111435882011818*I*J , K , L , -M,na.rm=TRUE)
    PA[t] <- FIN
