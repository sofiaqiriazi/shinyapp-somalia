    O<- max(126,sum( 4.0071261040803e-9*A*B*C , 4.5473706438896e-8*D*E*G*H , I , J , N , -1057.66177332745,na.rm=TRUE),na.rm=TRUE)
    FIN <-O
    PA[t] <- FIN
    PI[t] <- 0
