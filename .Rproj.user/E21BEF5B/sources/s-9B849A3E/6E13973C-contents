    I<- before.long[(t- 1),"Hiiraan_BeforeRegion"]
    J<- median(before.long[(t-6):(t- 1),"Gedo_BeforeRegion"], na.rm=TRUE)
    K<- current.long[(t- 6),"Bakool_CurrentRegion"]
    L<- future.long[(t- 14),"Mudug_FutureRegion"]
